{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-gzip": "ng build --prod --stats-json && gzipper --verbose ./dist",
    "build-prod-stats": "ng build --prod --stats-json",
    "test": "ng test --code-coverage --watch=false --progress=false --browsers ChromeHeadlessWithoutSandbox",
    "test-jest": "jest --coverage=true",
    "e2e": "ng e2e",
    "lint": "npm run ts-lint && npm run lint-style && npm run prettier",
    "lint-fix": "npm run ts-lint-fix && npm run lint-style-fix && npm run prettier-fix",
    "ts-lint": "ng lint",
    "ts-lint-fix": "ng lint --fix",
    "lint-style": "stylelint \"src/**/*.(scss|html)\"",
    "lint-style-fix": "stylelint \"src/**/*.(scss|html)\" --fix",
    "prettier": "prettier --config .prettierrc.js --list-different \"src/**/*.ts\"",
    "prettier-fix": "prettier --config --write .prettierrc.js --list-different \"src/**/*.ts\"",
    "serve": "node serve-script",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run frontend:server:production",
    "preinstall": "node ./scripts/check-dev-env nodeVersion=12.14 npmVersion=6.14.6",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats-es2015.json"
  },
  "pre-commit": [
    "lint-fix",
    "prettier-fix"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "~10.0.2",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/fire": "^5.2.3",
    "@angular/forms": "~10.0.4",
    "@angular/material": "^10.0.2",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/platform-server": "~10.0.4",
    "@angular/pwa": "^0.1000.3",
    "@angular/router": "~10.0.4",
    "@angular/service-worker": "~10.0.4",
    "@nestjs/common": "^7.3.2",
    "@nestjs/core": "^7.3.2",
    "@nestjs/ng-universal": "^3.2.0",
    "@nestjs/platform-express": "^7.3.2",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@nguniversal/express-engine": "^10.0.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "firebase": "7.16.1",
    "jspdf": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.3",
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-extensions/pretty-html-log": "^3.1.1",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@commitlint/config-conventional": "^9.1.1",
    "@ngrx/schematics": "^9.2.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.5",
    "@types/jspdf": "^1.3.3",
    "@types/node": "~14.0.23",
    "angular-configuration": "^0.5.0",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^8.6.0",
    "fuzzy": "^0.1.3",
    "gzipper": "^4.0.1",
    "inquirer": "^7.3.2",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "jasmine-auto-spies": "^5.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.1.0",
    "jest-given-when-then": "^1.0.1",
    "jest-jquery-matchers": "^2.1.0",
    "jest-preset-angular": "^8.2.1",
    "jquery": "^3.5.1",
    "ng-mocks": "^10.0.2",
    "nodemon": "^2.0.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rxjs-tslint-rules": "^4.34.0",
    "stylelint": "^13.6.1",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "~3.9.7",
    "wait-on": "^5.1.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
