{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage --watch=false --progress=false --browsers ChromeHeadlessWithoutSandbox",
    "e2e": "ng e2e",
    "lint": "npm run ts-lint && npm run lint-style && npm run prettier",
    "ts-lint": "ng lint",
    "ts-lint-fix": "ng lint --fix",
    "lint-style": "stylelint \"src/**/*.ts\"",
    "lint-style-fix": "stylelint \"src/**/*.ts\" --fix",
    "prettier": "prettier --config .prettierrc.js --list-different \"src/**/*.ts\"",
    "prettier-fix": "prettier --config --write .prettierrc.js --list-different \"src/**/*.ts\"",
    "preinstall": "node ./scripts/check-dev-env nodeVersion=10.16 npmVersion=6.11.2",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasmine-given": "^2.6.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-configuration": "^0.3.0",
    "angular-unit-component-driver": "^0.3.0",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-given": "^2.6.4",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-given": "^0.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-mocks": "^8.1.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "rxjs-tslint-rules": "^4.24.3",
    "stylelint": "^10.1.0",
    "stylelint-order": "^3.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "~3.5.3"
  }
}
