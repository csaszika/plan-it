{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-gzip": "ng build --prod --stats-json && gzipper --verbose ./dist",
    "build-prod-stats": "ng build --prod --stats-json",
    "test-jest": "jest --coverage=true",
    "lint": "npm run ts-lint && npm run lint-style && npm run prettier",
    "lint-fix": "npm run ts-lint-fix && npm run lint-style-fix && npm run prettier-fix",
    "ts-lint": "ng lint",
    "ts-lint-fix": "ng lint --fix",
    "lint-style": "stylelint \"src/**/*.(scss|html)\"",
    "lint-style-fix": "stylelint \"src/**/*.(scss|html)\" --fix",
    "prettier": "prettier --config .prettierrc.js --list-different \"src/**/*.ts\"",
    "prettier-fix": "prettier --config --write .prettierrc.js --list-different \"src/**/*.ts\"",
    "preinstall": "node ./scripts/check-dev-env nodeVersion=12.14 npmVersion=6.14.6",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats-es2015.json"
  },
  "pre-commit": [
    "lint-fix",
    "prettier-fix"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "~11.0.3",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/platform-server": "~11.0.5",
    "@angular/pwa": "^0.1100.5",
    "@angular/router": "~11.0.5",
    "@angular/service-worker": "~11.0.5",
    "@ngrx/effects": "^10.1.1",
    "@ngrx/store": "^10.1.1",
    "@ngrx/store-devtools": "^10.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "express": "^4.15.2",
    "firebase": "8.2.1",
    "jspdf": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1100.5",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-extensions/pretty-html-log": "^3.1.3",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@commitlint/config-conventional": "^11.0.0",
    "@ngrx/schematics": "^10.1.1",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/jspdf": "^1.3.3",
    "@types/node": "~14.14.14",
    "angular-configuration": "^0.5.0",
    "codelyzer": "^6.0.1",
    "firebase-tools": "^9.1.0",
    "fuzzy": "^0.1.3",
    "gzipper": "^4.3.0",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "jasmine-auto-spies": "^6.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jest": "^26.6.3",
    "jest-given-when-then": "^1.0.1",
    "jest-jquery-matchers": "^2.1.0",
    "jest-preset-angular": "^8.3.2",
    "jquery": "^3.5.1",
    "ng-mocks": "^11.2.6",
    "nodemon": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rxjs-tslint-rules": "^4.34.7",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "4.0.5",
    "wait-on": "^5.2.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0"
  }
}
