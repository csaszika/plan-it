{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage --watch=false --progress=false --browsers ChromeHeadlessWithoutSandbox",
    "e2e": "ng e2e",
    "lint": "npm run ts-lint && npm run lint-style && npm run prettier",
    "lint-fix": "npm run ts-lint-fix && npm run lint-style-fix && npm run prettier-fix",
    "ts-lint": "ng lint",
    "ts-lint-fix": "ng lint --fix",
    "lint-style": "stylelint \"src/**/*.ts\"",
    "lint-style-fix": "stylelint \"src/**/*.ts\" --fix",
    "prettier": "prettier --config .prettierrc.js --list-different \"src/**/*.ts\"",
    "prettier-fix": "prettier --config --write .prettierrc.js --list-different \"src/**/*.ts\"",
    "serve": "node serve-script",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run frontend:server:production",
    "preinstall": "node ./scripts/check-dev-env nodeVersion=10.16 npmVersion=6.11.2",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "~8.1.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.2.3",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/platform-server": "~8.2.3",
    "@angular/pwa": "^0.803.0",
    "@angular/router": "~8.2.3",
    "@angular/service-worker": "~8.2.3",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/ng-universal": "^2.0.1",
    "@nestjs/platform-express": "^6.0.0",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "@ngrx/store-devtools": "^8.3.0",
    "@nguniversal/express-engine": "^8.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "firebase": ">= 5.5.7 <7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/express": "^4.17.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasmine-given": "^2.6.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-configuration": "^0.3.0",
    "angular-unit-component-driver": "^0.3.0",
    "codelyzer": "^5.1.0",
    "firebase-tools": "^6.10.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-auto-spies": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-given": "^2.6.4",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-given": "^0.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-mocks": "^8.1.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "rxjs-tslint-rules": "^4.24.3",
    "stylelint": "^10.1.0",
    "stylelint-order": "^3.0.1",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "~3.5.3",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.1.0"
  }
}
